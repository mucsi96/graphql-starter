{
  "name": "graphql-starter",
  "version": "0.1.0",
  "description": "Starter project for GraphQL backend based on Node.js, Express, ES7 Async Await, MondoDB",
  "main": "src/server/index.js",
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "test": "cucumberjs --compiler js:babel-register test/server",
    "test:watch": "parallelshell \"esw src/server -w\" \"babel-watch node_modules/cucumber/bin/cucumber.js test/server\" ",
    "test:cover": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text cucumberjs --compiler js:babel-register test/server",
    "lint": "eslint src/server",
    "dev": "babel-watch src/server/index.js",
    "dev:watch": "parallelshell \"esw src/server -w\" \"npm run dev -s\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mucsi96/graphql-starter.git"
  },
  "keywords": [
    "graphql",
    "express",
    "es7",
    "async",
    "await",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mucsi96/graphql-starter/issues"
  },
  "homepage": "https://github.com/mucsi96/graphql-starter#readme",
  "dependencies": {
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-graphql": "^0.6.1",
    "graphql": "^0.8.1",
    "jsonwebtoken": "^7.1.9",
    "le-challenge-fs": "^2.0.8",
    "le-store-certbot": "^2.0.3",
    "letsencrypt-express": "^2.0.5",
    "mongodb": "^2.2.11",
    "pify": "^2.3.0",
    "redirect-https": "^1.1.0",
    "require-dir": "^0.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-watch": "^2.0.3",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "cucumber": "^1.3.1",
    "eslint": "^3.7.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-watch": "^2.1.14",
    "localhost.daplie.com-certificates": "^1.2.3",
    "mocha": "^3.1.2",
    "nyc": "^8.4.0",
    "parallelshell": "^2.0.0",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.2"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
